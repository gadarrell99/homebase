            "infrastructure": "GET /api/infrastructure",
            "agents": "GET /api/agents",
            "audit_results": "POST /api/audit/results"
        },
        "schedule": {
            "automated_audit": "0 3 * * * (3am ADT daily)",
            "manifest_refresh": "on-demand via Cortex or Claude Code",
            "learning_log_review": "after every audit run"
        },
        "files": {
            "talos": {
                "manifest_generator": "~/rize-prompts/manifest-generator.py",
                "cortex_fleet_router": "~/cortex/cortex_fleet_router.py",
                "architect_prompt": "~/rize-prompts/THE-ARCHITECT-v2.md",
                "inquisitor_prompt": "~/rize-prompts/THE-INQUISITOR-v2.md",
                "job1_agents": "~/rize-prompts/JOB1-AGENTS.md",
                "job2_apps": "~/rize-prompts/JOB2-APPS-DEMOS.md",
                "learning_log": "~/rize-prompts/learning-log.json",
                "fleet_manifest": "~/fleet-manifest.json",
                "audit_reports": "~/audit-reports/"
            }
        },
        "data_flow": [
            "1. Homebase serves infrastructure.json (fleet truth)",
            "2. Cortex manifest-generator pulls fleet from Homebase API",
            "3. Manifest generator SSHes to each server, discovers state",
            "4. Results written to ~/fleet-manifest.json",
            "5. Results pushed to Homebase /api/audit/results",
            "6. Cortex notified via /api/fleet/manifest-event",
            "7. Claude Code sessions query Cortex for ground truth",
            "8. Sentinel monitors pipeline health (see sentinel rules)"
        ],
        "sentinel_monitors": {
            "audit_freshness": "Alert if no audit report in 36 hours",
            "manifest_freshness": "Alert if fleet-manifest.json older than 36 hours",
            "learning_log_growth": "Warn if learning log unchanged for 7 days",
            "cortex_health": "Alert if Cortex :9100 unreachable",
            "homebase_api": "Alert if /api/infrastructure returns non-200",
            "compile_regression": "Alert if audit compile failures > 0"
        }
    }
